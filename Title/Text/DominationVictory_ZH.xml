        ss:AutoFitHeight="0"
        ss:Height="30"
      ><Cell><Data ss:Type="String">EVENT_FINAL_STAGE_TICK</Data></Cell><Cell
        ><Data ss:Type="String">ALLY</Data></Cell><Cell><Data
            ss:Type="String"
