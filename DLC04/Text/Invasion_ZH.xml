    <Cell><Data
            ss:Type="String"
          >The Beacon shield has been damaged!</Data></Cell>
    <Cell><Data ss:Type="String">信标护盾受到了伤害！</Data></Cell>
    <Cell ss:Index="7"><Data ss:Type="String">Name of an event</Data></Cell>
   </Row>
