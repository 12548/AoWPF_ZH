        ss:AutoFitHeight="0"
        ss:Height="45"
      ><Cell><Data ss:Type="String">VICTORY_CONDITION</Data></Cell><Cell><Data
            ss:Type="String"
          >NAME</Data></Cell><Cell><Data
            ss:Type="String"
          >Invasion </Data></Cell><Cell><Data
            ss:Type="String"
